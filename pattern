#include <stdio.h>
#include <stdlib.h>
int main()
{
    int i,j;
    for(i=0; i<=6; i++)
	{
        for(j=1; j<=i+2; j++)
		{
            if(j%2==1)
            {
                printf("0");
            }
            else
			{
                printf("1");
            }
        }
        printf("\n");
    }
    getch();
    return 0;
}
